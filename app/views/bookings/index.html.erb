<div class= container>
<h1><strong> All the bookings</strong></h1>
<div class=cards>
<% @bookings.each do |booking| %>
    <div class="card-product">
      <img src= <%= booking.boat.picture_url %> />
      <div class="card-product-infos">
        <h2>Réservation pour <%= link_to booking.boat.name, booking_path(booking) %> </h2>
        <ul>
        <li>Date d'arrivée <%= booking.begin_date %></li>
        <li>Date de fin <%= booking.end_date %></li>
        </ul>
      </div>
    </div>
<% end %>
</div>
</div>
